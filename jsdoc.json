{
  "source": {
    "include": ["src"],
    "includePattern": ".+\\.tsx?$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": ["plugins/markdown"],
  "opts": {
    "recurse": true,
    "destination": "./docs"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
} 