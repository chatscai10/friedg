{
  "name": "web-admin",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc -b && vite build",
    "build:testing": "tsc -b && vite build --mode testing",
    "deploy:testing": "npm run build:testing && firebase deploy --only hosting",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --fix",
    "preview": "vite preview",
    "test": "jest",
    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,html}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,html}\""
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.15.0",
    "@mui/material": "^5.15.0",
    "dayjs": "^1.11.11",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.24.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@firebase/rules-unit-testing": "^4.0.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.14.9",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "@vitejs/plugin-react": "^4.3.1",
    "depcheck": "^1.4.7",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.5",
    "express": "^5.1.0",
    "firebase": "^11.6.1",
    "firebase-admin": "^13.3.0",
    "jsdoc": "^4.0.4",
    "jsdom": "^26.1.0",
    "prettier": "^3.5.3",
    "typescript": "^5.2.2",
    "vite": "^5.3.1",
    "vitest": "^3.1.2"
  }
}
