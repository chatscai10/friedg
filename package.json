    "eslint-plugin-react-refresh": "^0.4.7",
    "firebase": "^11.6.1",
    "firebase-admin": "^13.3.0",
    "globals": "^16.0.0",
    "jsdom": "^26.1.0",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.26.1",
    "vite": "^5.3.1",
    "vitest": "^3.1.2"
  }
